{
  "name": "web-seed",
  "version": "0.0.0",
  "homepage": "https://github.com/JonnyBGod/web-seed",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --build-optimizer",
    "test": "ng test",
    "lint": "ng lint --type-check",
    "e2e": "ng e2e",
    "build:docker": "docker build -t web-seed:latest .",
    "ci": "ng lint --type-check && ng test --single-run && ng e2e",
    "git:merge": "git fetch upstream && git merge upstream/master --allow-unrelated-histories",
    "git:merge:preview": "git fetch upstream && git merge --no-commit upstream/master --allow-unrelated-histories",
    "git:prepare": "git add . && git add -f tools/install.js && git commit -m'setup'",
    "git:setup": "git init && git remote add upstream https://github.com/JonnyBGod/web-seed.git",
    "github-deploy": "ng build --prod --build-optimizer --environment github --base-href \"/web-seed/\" && angular-cli-ghpages",
    "serve:prod": "static dist -a '0.0.0.0' -c false --spa",
    "start:local": "ng serve --environment devLocal"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JonnyBGod/web-seed.git"
  },
  "bugs": {
    "url": "https://github.com/JonnyBGod/web-seed/issues"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.3.6",
    "@angular/common": "^4.3.6",
    "@angular/compiler": "^4.3.6",
    "@angular/core": "^4.3.6",
    "@angular/forms": "^4.3.6",
    "@angular/http": "^4.3.6",
    "@angular/platform-browser": "^4.3.6",
    "@angular/platform-browser-dynamic": "^4.3.6",
    "@angular/platform-server": "^4.3.6",
    "@angular/router": "^4.3.6",
    "@angularclass/conventions-loader": "^1.0.13",
    "@angularclass/hmr": "^2.1.3",
    "@angularclass/hmr-loader": "^3.0.4",
    "@cloudtasks/ngx-image": "^1.0.1",
    "@ngrx/effects": "^4.0.5",
    "@ngrx/router-store": "^4.0.4",
    "@ngrx/store": "^4.0.3",
    "@ngx-config/core": "^4.0.0",
    "@ngx-translate/core": "^8.0.0",
    "@ngx-translate/http-loader": "^1.0.2",
    "@swimlane/ngx-datatable": "^10.2.1",
    "angular2-notifications": "^0.7.7",
    "angulartics2": "^2.3.0",
    "bootstrap": "^4.0.0-beta",
    "core-js": "^2.5.1",
    "font-awesome": "^4.7.0",
    "ie-shim": "^0.1.0",
    "immutable": "^3.8.1",
    "lodash-es": "^4.17.4",
    "loopback-filters": "^0.1.3",
    "ng2-file-upload": "^1.2.1",
    "ng2-select": "^1.2.0",
    "ngrx-store-logger": "^0.2.0",
    "ngx-bootstrap": "^1.9.1",
    "ngx-chips": "^1.5.3",
    "ngx-scrollspy": "^1.0.0",
    "ngx-stripe": "^0.2.0",
    "reflect-metadata": "^0.1.10",
    "reselect": "^3.0.1",
    "rxjs": "^5.4.3",
    "socket.io-client": "^2.0.3",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "@angular/cli": "^1.3.2",
    "@angular/compiler-cli": "^4.3.6",
    "@ngrx/store-devtools": "^4.0.0",
    "@types/jasmine": "^2.5.54",
    "@types/node": "^8.0.27",
    "@types/stripe": "^3.0.0",
    "angular-cli-ghpages": "^0.5.1",
    "codelyzer": "^3.1.2",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngrx-store-freeze": "^0.1.9",
    "node-sass": "^4.5.3",
    "node-static": "^0.7.9",
    "protractor": "~5.1.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2"
  }
}
